{
  "name": "donga-website",
  "version": "1.0.0",
  "description": "Donga website - SCSS compilation",
  "scripts": {
    "build:css": "sass assets/scss/main.scss:assets/css/main.css --style=expanded --source-map",
    "watch:css": "sass --watch assets/scss/main.scss:assets/css/main.css --style=expanded --source-map",
    "build:css:compressed": "sass assets/scss/main.scss:assets/css/main.css --style=compressed --source-map"
  },
  "devDependencies": {
    "sass": "^1.69.0"
  }
}



